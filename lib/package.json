    },
    "peerDependencies": {
        "luxon": "^3.6.1",
        "vue": "^3.5.7"
    },
    "devDependencies": {
        "@tsconfig/node22": "^22.0.2",
        "@types/jsdom": "^21.1.7",
        "@types/luxon": "^3.6.2",
