    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/windvane.es.js",
            "require": "./dist/windvane.umd.js"
        },
        "./windvane.css": "./dist/windvane.css",
        "./global": {
            "types": "./dist/global.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "global": [
                "./dist/global.d.ts"
            ]
        }
