        "@vue/tsconfig": "^0.7.0",
        "eslint": "^9.29.0",
        "eslint-plugin-vue": "~10.2.0",
        "jiti": "^2.4.2",
        "jsdom": "^26.1.0",
        "npm-run-all2": "^8.0.4",
        "prettier": "3.5.3",
        "typescript": "~5.8.0",
        "unplugin-auto-import": "^19.3.0",
        "vite": "^7.0.0",
        "vite-plugin-dts": "^4.5.4",
        "vite-plugin-vue-devtools": "^7.7.7",
        "vitest": "^3.2.4",
        "vue-tsc": "^2.2.10"
    }
